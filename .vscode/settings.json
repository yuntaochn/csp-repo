{
    "editor.fontSize": 20,
    "cmake.configureOnOpen": false,
    // Doxygen documentation generator set
    "doxdocgen.file.copyrightTag": [
        "@copyright Copyright (c) {year}  德州安丰机械制造有限公司"
    ],
    "doxdocgen.file.customTag": [
        "@par 修改日志:",
        "<table>",
        "<tr><th>Date       <th>Version <th>Author  <th>Description",
        "<tr><td>{date} <td>1.0     <td>wangh     <td>内容",
        "</table>",
    ],
    "doxdocgen.file.fileOrder": [
        "copyright",
        "empty",
        "author",
        "version",
        "date",
        "empty",
        "file",
        "brief"
    ],
    "doxdocgen.file.fileTemplate": "@file {name}",
    "doxdocgen.file.versionTag": "@version 0.1",
    "doxdocgen.generic.authorEmail": "yuntaochn@gmail.com",
    "doxdocgen.generic.authorName": "Zhang Yuntao",
    "doxdocgen.generic.authorTag": "@author {author} ({email})",

    "doxdocgen.generic.order": [
        "brief",
        "tparam",
        "param",
        "return"
    ],
    "doxdocgen.generic.paramTemplate": "@param{indent:8}{param}{indent:25}",
    "doxdocgen.generic.returnTemplate": "@return {type} ",
    "doxdocgen.generic.splitCasingSmartText": true,
    "doxdocgen.c.firstLine": "/*********************************************************************************",
    "doxdocgen.c.lastLine": " ********************************************************************************/",
    "C/C++ Include Guard.Macro Type": "Filename",
    "C/C++ Include Guard.Suffix": "_H",
}